                </div>
            </div>

            <div class="pic">
                <img src="~/images/hero-img.png" alt="chicken curry">
            </div>
        </div>
    </div>
    @*----------------------------------------------*@
    <div>
        @RenderBody()
    </div>
    @*----------------- chefs---------------------------*@
        <div id="chefs" class="chefs">
